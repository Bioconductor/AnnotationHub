Package: AnnotationHub
Type: Package
Title: Client to access AnnotationHub resources
Version: 2.17.6
Authors@R: 
    c(person("Martin", "Morgan", email="martin.morgan@roswellpark.org",
             role="cre"),
      person("Marc", "Carlson", role="ctb"),
      person("Dan", "Tenenbaum", role="ctb"),
